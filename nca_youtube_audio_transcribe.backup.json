      "position": [
        -1220,
        -80
      ],
      "parameters": {
        "path": "nca-transcribe",
        "options": {
          "responseData": "allEntries",
          "responseCode": 200,
          "responseMode": "onReceived",
          "responseDataType": "json"
        }
      }
    },
    {
      "id": "5a4f1f8f-1a3e-4a1e-b9b4-1e2b0a91a3e7",
      "name": "üñêÔ∏è Manual Trigger (Testing)",
      "type": "n8n-nodes-base.manualTrigger",
      "typeVersion": 1,
      "position": [
        -1220,
        120
      ],
      "parameters": {}
    },
    {
      "id": "3c4b2e91-7d8d-4f92-9a41-1b9b7e55a2c3",
